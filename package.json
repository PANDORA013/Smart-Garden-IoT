{
    "$schema": "https://www.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:network": "vite --host 0.0.0.0",
        "serve:network": "php artisan serve --host=0.0.0.0 --port=8000",
        "start:network": "concurrently \"npm run dev:network\" \"npm run serve:network\" --names \"VITE,LARAVEL\" --prefix-colors \"cyan,magenta\""
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^5.1.2",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.7"
    },
    "dependencies": {
        "lucide-react": "^0.554.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    }
}
